{
    "name": "maimaidx-prober-updater-v2",
    "version": "1.0.0",
    "description": "V2 Service for maimaidx-prober-updater",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "start": "cross-env NODE_ENV=production node --enable-source-maps --experimental-strip-types src/index.ts",
        "start:dev": "cross-env NODE_ENV=dev DUMP_FRIEND_VS_HTML=1 node --watch --watch-path=.env --watch-path=config.json --watch-path=src --enable-source-maps --experimental-strip-types src/index.ts"
    },
    "engines": {
        "node": ">=22.6.0"
    },
    "dependencies": {
        "cross-env": "^7.0.3",
        "express": "^4.18.1",
        "fetch-cookie": "^3.1.0",
        "http-parser-js": "^0.5.8",
        "lodash": "^4.17.21",
        "tough-cookie": "^6.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/lodash": "^4.14.191",
        "@types/node": "^18.11.18",
        "@types/tough-cookie": "^4.0.5",
        "node": "^22.6.0",
        "typescript": "^5.5.4"
    }
}